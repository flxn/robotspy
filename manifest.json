{
  "name" : "RobotSpy",
  "version" : "0.1",
  "description" : "View what robots are not allowed to see.",
  "permissions": [ "tabs", "http://*/", "https://*/" ],
  "browser_action": {
     "default_icon": "icon48.png",
     "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "manifest_version": 2
}
